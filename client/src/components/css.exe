.main-container {
    display: flex;
    justify-content: flex-end;
    /* align-items: center; */
    height: 100vh;
    width: 100%;
  }
.section.content-grid{
    flex: 1.5;
}
  .image-grid {
    background-color: black;
    flex: 1;
    display: grid;
    grid-template-columns: repeat(3, 1fr); /* 3 columns */
    grid-auto-rows: 250px; /* Fixed height for each grid item */
    overflow: hidden; /* Hide scrollbar */
    position: relative; /* Ensure proper positioning */
    height: 100%; /* Full height */
    gap: 0; /* Remove any gaps between grid items */
    /* width: 80%; 
    max-width: 1200px;  */
    /* margin: 0 auto;  */
  }
  
  .grid-item {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 10px; /* Add padding for aesthetics */
  }
  
  .grid-item img {
    width: 100%;
    height: 100%;
    object-fit: cover; /* Ensures the image covers the entire grid item */
    margin: 0; /* Ensure no margin on images */
    padding: 0; /* Ensure no padding on images */
  }
  
  /* Specific spans for images */
  .span-2-columns {
    grid-column: span 2; /* Span 2 columns */
  }
  
  .spacer {
    flex: 0.5; /* Adjust spacer size */
  }
  
  .text-container {
    flex: 2;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 20px;
    background-color: rgb(226, 161, 63);
  }
  
  .text-container h2,
  .text-container p {
    margin: 0;
  }
  
























































  /* General container for the about page */
.about-page {
    display: flex;
    flex-direction: row; /* Stack all sections vertically */
    width: 100%;
  }
  
  /* Spacer section styling */
  .spacer-section {
    flex: 0.5; /* This flex value can be adjusted */
    background-color: #000;
    height: 100vh; /* Full viewport height */
    width: 100%;
    /* z-index: 999; */
  }
  
  /* Wrapper for heading and content to keep them together */
  .main-about-content {
    flex: 3.5;
    display: flex;
    flex-direction: column; /* Stack heading above content */
    width: 100%;
    align-items: center;
  }
  
  /* Heading section styling */
  .heading-section {
    flex: 0.5;
    display: flex;
    flex-direction: row;
    align-items: center;
 
    justify-content: space-between;
    width: 80%;
    max-width: 1200px;
    /* margin-bottom: 20px; */
    background-color:#000;
    text-align: center;
    /* gap: 20px; */
    /* padding: 1rem; */
      min-height: 10%;
  }
  
  .heading-section h1 {
    font-size: 3.5rem;
    margin: 0;
    color: #fff;
  }
  .heading-section p{
    align-self: last baseline;
    color: #fff;
  }

  /* Main content section styling */
  .content-section {
    /* background-color: #ca1a1a; */
    flex: 2; /* This will take the remaining space */
    display: flex;
    align-self: flex-start;
    justify-self: flex-start;
    flex-direction: row-reverse; /* Ensure photo and text order */
    width: 100%;
    align-items: center;
    position: relative;
  }
  
  /* Styling for the photo container */
  .about-photo {
    display: flex;
    flex-direction: column; /* Stack items vertically */
    justify-content: flex-start; /* Center vertically */
    align-items: flex-start; /* Center horizontally */
    background-color: blue;
    z-index: 1000;
    /* You can add height if necessary */
    height: 60vh; /* Set a specific height for the container if needed */
    width: 300px; /* Adjust width as necessary */
}
  
  .about-photo img {
    width: 90%;
    max-width: 300px; 
    height: 90%;
    max-height: 50vh;
    object-fit: contain; /* Adjust this based on your needs */
}

  
  /* Styling for the text container */
  .about-text {
    position: absolute;
    top: 5rem;
    right: 250px;

    background-color:yellow;
    flex: 2;
    width: 60vw;
    min-height: 100vh;
    /* z-index: 0; */
    padding: 50px;
    /* padding-right: 70px; */
    text-align: left;
  }
  
  .about-text h2 {
    font-size: 2rem;
    margin-bottom: 10px;
  }
  
  .about-text p {
    font-size: 1rem;
    line-height: 1.6;
    color:#000;
  }
  









































  












































































































  .projects-container {
    display: flex;
    justify-content: center;
    align-items: flex-start;
}

.spacer {
    flex: 0.5;
    background-color: #000;
    height: 100vh;
    z-index: 999;
    border: 2px solid #fff;
}

.projects-content {
    background-color: #ffffff;
    /* background-image: url(../assets/landing1.png); */
    flex: 3.5;
    display: flex;
    height: 100vh;
    width: 100vw;
    background-repeat: no-repeat;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
    gap: 2rem;
}

.projects-header {
    /* align-self: flex-start; */
    justify-self: flex-start;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    background-color:transparent;
    max-width: 80%;
    width: 80%;
    max-height: 20vh;
    height: 15vh;
    /* text-align: center; */
    /* margin-bottom: 2rem; */
}

.projects-header h2 {
    font-size: 3rem;
    color:#000;
    border-bottom:2px solid #000;
}

.projects-header p {
    align-self: flex-end;
    justify-self: flex-end;
    font-size: 1.02rem;
    color:#6a6363;
    /* border-bottom:2px solid #fff; */
}

/* 
.projects-grid {
    align-items: start; 
    max-width: 80%;
   
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
   
    grid-auto-rows: 0.5;
    align-items: stretch;
    align-content: stretch;
    gap: 1.5rem;
} */


.projects-grid {
    display: flex;
    /* flex-wrap: wrap;  */
    max-width: 80%; /* Restricts max-width */
    gap: 1.5rem;
    align-items: center;
    justify-content: center; /* Centers the items */
}


.project-card {
    flex: 1;
    display: flex;
    flex-direction: column-reverse;
    padding: 1rem;
    background-color:#c6a29e25;
    gap: 0.5rem;
    /* border: 2px solid #000; */
    align-self: flex-start;
    /* transition: transform 0.5s ease, box-shadow 0.3s ease; */
    opacity: 1; /* Set to 1 for visibility */
    /* transform: translateY(0); */
    /* will-change: transform; */
    /* animation: slideUp 1s ease-in-out forwards;  */
    border-radius: 10px;
}
.project-info{
    display: flex;
    flex-direction: column;
    gap: 1rem;
    font-size: 0.8rem;
}
.project-info h3{
    /* font-weight: 900; */
    /* color: aqua; */
    font-size: 2rem;
}
.project-card:hover {
    border: 2px solid #000;
    transform: scale(1.3); /* Smaller scale effect */
    box-shadow: 0 4px 20px rgba(255, 255, 255, 0.2); /* Shadow on hover */
}

/* Project-specific styles */
.project-images{
    display: flex;
    justify-content: center;
    overflow: hidden;
    gap: 0.5rem;
}
.project-images img,
.project-images img,
.project-images img {
    width: 100%; /* Ensures image spans full width of the container */
    max-height: 200px; /* Sets a maximum height for the image */
    object-fit: cover; /* Maintains aspect ratio and crops excess */
    border-radius: 4px;
}

.project-links {
    display: flex;
    justify-content: center;
    gap: 0.5rem;
    /* margin-top: 0.5rem; */
}

.project-links a {
    color: #000;
    transition: color 0.3s ease;
}

.project-links a:hover {
    color: #0073e6; /* Change color on hover for effect */
}




/* Project.css */

  /* Animation for slide-up effect */
  @keyframes slideUp {
    0% {
        opacity: 0; /* Start hidden */
        transform: translateY(500px); /* Start from below */
    }
    100% {
        opacity: 1; /* Fully visible */
        transform: translateY(0); /* End position */
    }
}

/* Adding a uniform delay for all project cards */
.project-card:nth-child(1) { animation-delay: 0.2s; }
.project-card:nth-child(2) { animation-delay: 0.3s; }
.project-card:nth-child(3) { animation-delay: 0.4s; }
.project-card:nth-child(4) { animation-delay: 0.5s; }
.project-card:nth-child(5) { animation-delay: 0.6s; }



/* Adjustments for smaller screens */
@media (max-width: 760px) {
    .projects-grid {
        display: block;
        
    }
}




















































